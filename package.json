{
  "private": true,
  "name": "saasy-land-next-13-starters-with-authentication-and-database-implemented",
  "description": "Starter templates for Next.js 13 full-stack projects. Built with Tailwind CSS, ShadCn, authentication, and database. Several branches contain several different configurations, including serverless databases like PostgreSQL with Neon, or MySQL with PlanetScale, Drizzle ORM, Prisma ORM v.5 (serverless-compatible), but also MongoDB and Supabase.",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Piotr Borowiecki",
    "url": "https://pjborowiecki.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pjborowiecki/SAASY-LAND-Next-13-Starters-With-Authentication-And-Database-Implemented/tree/next-auth-drizzle-planetscale-mysql"
  },
  "bugs": {
    "url": "https://github.com/pjborowiecki/SAASY-LAND-Next-13-Starters-With-Authentication-And-Database-Implemented/issues"
  },
  "scripts": {
    "dev": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "postinstall": "drizzle-kit generate:mysql --config=drizzle.config.ts && drizzle-kit push:mysql --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate:mysql --config=drizzle.config.ts",
    "db:push": "drizzle-kit push:mysql --config=drizzle.config.ts",
    "db:pull": "drizzle-kit introspect:mysql --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:check": "drizzle-kit check:mysql --config=drizzle.config.ts",
    "db:update": "drizzle-kit up --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --port 4000 --verbose",
    "shadcn:add": "pnpm dlx shadcn-ui@latest add",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@auth/core": "^0.17.0",
    "@auth/drizzle-adapter": "^0.3.5",
    "@hookform/resolvers": "^3.3.2",
    "@planetscale/database": "^1.11.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-email/components": "^0.0.7",
    "@t3-oss/env-nextjs": "^0.7.1",
    "bcrypt": "^5.1.1",
    "bufferutil": "^4.0.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.2",
    "next": "13.5.4",
    "next-auth": "^4.24.3",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^1.9.5",
    "react-hook-form": "^7.47.0",
    "resend": "^1.1.0",
    "sonner": "^1.0.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "undici": "^5.26.5",
    "utf-8-validate": "^6.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/eslint": "^8.44.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.8",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.52.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
